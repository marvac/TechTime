<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/jquery.tablesorter/dist/css/theme.bootstrap_3.min.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/lib/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/lib/metisMenu/dist/metisMenu.css" rel="stylesheet" />
    <link href="~/lib/morrisjs/morris.css" rel="stylesheet" />
    <link href="~/lib/jquery.tablesorter/dist/css/jquery.tablesorter.pager.min.css" rel="stylesheet" />
    <link href="//cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/bootstrap3-editable/css/bootstrap-editable.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

    @RenderSection("styles", false)

    <title>ADS TechTime - @ViewBag.Title</title>
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand">ADS TechTime</a>
                <ul class="nav navbar-top-links navbar-right">
                    <li>
                        <a onclick="print('printable')">
                            <i class="fa fa-print"></i>
                        </a>
                    </li>
                </ul>
            </div>

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a asp-controller="Home" asp-action="Index"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                        </li>
                        <li>
                            <a asp-controller="Home" asp-action="Create"><i class="fa fa-pencil fa-fw"></i> Create Work Entry</a>
                        </li>
                        <li>
                            <a asp-controller="Home" asp-action="History"><i class="fa fa-align-center fa-fw"></i> View History</a>
                        </li>
                        <li>
                            <a asp-controller="Home" asp-action="Contact"><i class="fa fa-envelope fa-fw"></i> Contact Carl</a>
                        </li>
                        <li>
                            <a class="text-danger" asp-controller="Auth" asp-action="Logout"><i class="fa fa-sign-out fa-fw text-danger"></i> Logout</a>
                        </li>

                    </ul>
                </div>

            </div>

        </nav>
        <div id="page-wrapper">
            <div class="row" id="printable">
                <div class="col-md-12">
                    <h2 class="page-header">@ViewBag.Title</h2>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/jquery.tablesorter/dist/js/jquery.tablesorter.min.js"></script>
    <script src="~/lib/jquery.tablesorter/dist/js/jquery.tablesorter.widgets.min.js"></script>
    <script src="~/lib/jquery.tablesorter/dist/js/extras/jquery.tablesorter.pager.min.js"></script>
    <script src="~/lib/metisMenu/dist/metisMenu.js"></script>
    <script src="~/lib/startbootstrap-sb-admin-2/dist/js/sb-admin-2.js"></script>
    <script src="~/lib/raphael/raphael.min.js"></script>
    <script src="~/lib/morrisjs/morris.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/bootstrap3-editable/js/bootstrap-editable.min.js"></script>

    @RenderSection("scripts", false)
</body>
</html>
